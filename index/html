                correct: ['üéµ –°–ø–æ–∫–æ–π–Ω–∞—è –º—É–∑—ã–∫–∞', 'üßò‚Äç‚ôÄÔ∏è –†–µ–ª–∞–∫—Å–∞—Ü–∏—è', 'üõÅ –¢–µ–ø–ª–∞—è –≤–∞–Ω–Ω–∞'],
                wrong: ['‚òï –ö–æ—Ñ–µ', 'üì± –°–æ—Ü—Å–µ—Ç–∏', 'üé¨ –§–∏–ª—å–º —É–∂–∞—Å–æ–≤', '‚ùå –ù–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞—Ç—å'],
                sound: () => {
                    playHumanExcitedSound();
                    setTimeout(() => playComplexSound([200, 150], [1, 1.2], ['sine', 'sine'], [0.04, 0.03]), 800);
                },
                emotion: 'excited'
            },
            meteor: {
                title: '‚òÑÔ∏è –ú–µ—Ç–µ–æ—Ä–∏—Ç–Ω—ã–π –¥–æ–∂–¥—å',
                symptoms: '–ü–µ—Ä—Å–æ–Ω–∞–∂ —Å—Ç—Ä–∞–¥–∞–µ—Ç –æ—Ç –±–µ—Å—Å–æ–Ω–Ω–∏—Ü—ã –∏ –ø–µ—Ä–µ–ø–∞–¥–æ–≤ –Ω–∞—Å—Ç—Ä–æ–µ–Ω–∏—è...',
                correct: ['üò¥ –†–µ–∂–∏–º —Å–Ω–∞', 'üßò‚Äç‚ôÄÔ∏è –ú–µ–¥–∏—Ç–∞—Ü–∏—è', 'üö∂‚Äç‚ôÄÔ∏è –õ–µ–≥–∫–∏–µ –ø—Ä–æ–≥—É–ª–∫–∏'],
                wrong: ['üéÆ –ò–≥—Ä—ã –≤—Å—é –Ω–æ—á—å', '‚òï –≠–Ω–µ—Ä–≥–µ—Ç–∏–∫–∏', 'üì∫ –¢–µ–ª–µ–≤–∏–∑–æ—Ä', '‚ùå –ù–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞—Ç—å'],
                sound: () => {
